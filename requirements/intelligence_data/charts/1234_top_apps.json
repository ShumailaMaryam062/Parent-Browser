{
  "type": "bar",
  "labels": [
    "Chrome Browser",
    "YouTube"
  ],
  "datasets": [
    {
      "label": "Usage Count",
      "data": [
        2,
        1
      ],
      "backgroundColor": [
        "#6b7280",
        "#6b7280"
      ],
      "borderColor": [
        "#4b5563",
        "#4b5563"
      ],
      "borderWidth": 2,
      "borderRadius": 6,
      "borderSkipped": false
    }
  ],
  "options": {
    "responsive": true,
    "maintainAspectRatio": false,
    "plugins": {
      "legend": {
        "display": false
      },
      "title": {
        "display": true,
        "text": "Top Applications by Usage",
        "font": {
          "size": 18,
          "weight": "bold"
        },
        "color": "#c7d2fe",
        "padding": 20
      },
      "tooltip": {
        "backgroundColor": "rgba(30, 27, 75, 0.95)",
        "titleFont": {
          "size": 14,
          "weight": "bold"
        },
        "bodyFont": {
          "size": 13
        },
        "padding": 12,
        "cornerRadius": 10,
        "borderColor": "rgba(139, 92, 246, 0.5)",
        "borderWidth": 2
      }
    },
    "scales": {
      "y": {
        "beginAtZero": true,
        "grid": {
          "color": "rgba(139, 92, 246, 0.1)",
          "lineWidth": 1
        },
        "ticks": {
          "font": {
            "size": 11
          },
          "color": "#a5b4fc"
        }
      },
      "x": {
        "grid": {
          "display": false
        },
        "ticks": {
          "font": {
            "size": 11
          },
          "color": "#a5b4fc",
          "maxRotation": 45,
          "minRotation": 45
        }
      }
    },
    "animation": {
      "duration": 1200,
      "easing": "easeOutQuart"
    }
  }
}